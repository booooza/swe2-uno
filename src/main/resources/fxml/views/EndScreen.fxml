<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<StackPane fx:id="root"
           xmlns:fx="http://javafx.com/fxml/1"
           xmlns="http://javafx.com/javafx/2.2">
    <children>
        <Button layoutX="211.0" layoutY="330.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0"
                fx:id="endButton"
                text="Exit"/>
        <Button layoutX="248.0" layoutY="330.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0"
                fx:id="restartButton"
                text="Restart"/>
        <Label fx:id="message" layoutX="211.0" layoutY="54.0" text="EndMessage" textFill="WHITE">
            <font>
                <Font name="Helvetica" size="18.0"/>
            </font>
        </Label>
        <ImageView fx:id="imgView" layoutX="223.0" layoutY="120.0"/>
    </children>

</StackPane>
