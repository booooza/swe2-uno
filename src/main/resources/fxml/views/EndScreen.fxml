<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<?import com.jfoenix.controls.JFXButton?>
<StackPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: red">
            <children>
                <Label fx:id="message" layoutX="250.0" layoutY="52.0" text="EndMessage">
                    <font>
                        <Font name="Helvetica" size="18.0" />
                    </font>
                </Label>

                <ImageView fx:id="imgView" fitHeight="174.0" fitWidth="330.0" layoutX="135.0" layoutY="113.0" />

                <JFXButton fx:id="restartButton" buttonType="RAISED" layoutX="14.0" layoutY="296.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0" text="Restart" />
                <JFXButton fx:id="endButton" buttonType="RAISED" layoutX="476.0" layoutY="296.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="110.0" text="Exit" />


            </children>
        </AnchorPane>
    </children>
</StackPane>
